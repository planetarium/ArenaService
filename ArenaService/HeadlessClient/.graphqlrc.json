{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "HeadlessClient",
      "namespace": "ArenaService.Client",
      "url": "https://9c-main-rpc-1.nine-chronicles.com/graphql",
      "dependencyInjection": true,
      "records": {
        "inputs": false,
        "entities": false
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}